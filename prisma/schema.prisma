import { PrismaClient } from '@prisma/client';

const prisma = new PrismaClient();

// Now you can use prisma to interact with your database

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Videoupload{
    id String @id @default(uuid()) @map("_id")
    name String?
    url String
}

